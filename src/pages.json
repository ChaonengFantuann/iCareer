{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app123",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/course/index",
			"style":{
				"navigationBarTitleText":"课程",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/learning/index",
			"style":{
				"navigationBarTitleText":"学习",
				"navigationStyle":"custom",
				"app-plus":{
					"scrollIndicator":"none"
				}
			}
		},
		{
			"path":"pages/message/index",
			"style":{
				"navigationBarTitleText":"消息",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/user/index",
			"style":{
				"navigationBarTitleText":"我的",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/login/loginIndex",
			"style":{
				"navigationBarTitleText":"登录首页",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/login/loginPassword",
			"style":{
				"navigationBarTitleText":"密码登录",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/login/passwordSet",
			"style":{
				"navigationBarTitleText":"密码设置",
				"navigationStyle":"custom"
			}
		},
		{
			"path":"pages/user/userInformation",
			"style":{
				"navigationBarTitleText":"用户信息",
				"navigationStyle":"custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar":{
		"color": "#4A4747",
		"selectedColor": "#45818E",
		"borderStyle":"black",
		"fontSize":"14px",
		"height":"52px",
		"list":[
			{
                "pagePath":"pages/index/index",
			    "iconPath":"static/index.png",
			    "selectedIconPath":"static/index-color.png",
			    "text":"主页"
			},
			{
				"pagePath":"pages/course/index",
				"iconPath":"static/course.png",
				"selectedIconPath":"static/course-color.png",
				"text":"课程"
			},
			{
				"pagePath":"pages/learning/index",
				"iconPath":"static/learn.png",
				"selectedIconPath":"static/learn-color.png",
				"text":"学习"
			},
			{
				"pagePath":"pages/message/index",
				"iconPath":"static/message.png",
				"selectedIconPath":"static/message-color.png",
				"text":"消息"
			},
			{
				"pagePath":"pages/user/index",
				"iconPath":"static/user.png",
				"selectedIconPath":"static/user-color.png",
				"text":"我的"
			}
		]
	}
}
